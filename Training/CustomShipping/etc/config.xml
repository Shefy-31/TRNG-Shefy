<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <customshipping>
                <active>1</active>
                <allowed_methods>delivery</allowed_methods>
                <methods>delivery</methods>
                <model>Ceymox\CustomShipping\Model\Carrier</model>
                <name>Free Shipping within Kerala</name>
                <title>Free Shipping within Kerala</title>
            </customshipping>
        </carriers>
    </default>
</config>